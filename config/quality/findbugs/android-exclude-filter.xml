<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class title="~.*\.R\$.*"/>
    </Match>
    <Match>
        <Class title="~.*\.Manifest\$.*"/>
    </Match>
    <!-- All bugs in test classes, except for JUnit-specific bugs -->
    <Match>
        <Class title="~.*\.*Test" />
        <Not>
            <Bug code="IJU" />
        </Not>
    </Match>


</FindBugsFilter>